<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taverncat2020">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone_provisorio"
        android:label="@string/app_name"
        android:windowSoftInputMode="adjustPan"
        android:roundIcon="@mipmap/icone_provisorio_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TavernCat">
        <activity
            android:name="com.example.taverncat2020.Cardactivity"
            android:label="@string/app_name" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>